<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" prefHeight="465.0" prefWidth="397.0"
      xmlns="http://javafx.com/javafx/8.0.121" style="-fx-font-family:'Arial Narrow'"
      fx:controller="com.fstg.caissev2.controllers.ProduitSearch">
   <children>
             <GridPane hgap="10" vgap="10">
                 <children>
                     <Label GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="3">Selectionnez un
                         produit pour une modification :
                     </Label>
                     <Label GridPane.columnIndex="0" GridPane.rowIndex="1">Categorie:</Label>
                     <ComboBox fx:id="categorieComboBox" layoutX="49.0" layoutY="28.0" prefHeight="25.0"
                               prefWidth="144.0" GridPane.columnIndex="1" GridPane.rowIndex="1"
                               onMouseClicked="#initCategorieComboBoxItems"/>
                     <Button fx:id="search" onAction="#searchProduct" prefHeight="25.0" prefWidth="64.0"
                             GridPane.columnIndex="2" GridPane.rowIndex="1">Search
                     </Button>
                 </children>
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
         </rowConstraints>
             </GridPane>
       <TableView fx:id="tableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                  minHeight="-Infinity" minWidth="-Infinity" onMouseClicked="#showAlert">
         <VBox.margin>
            <Insets top="3.0" />
         </VBox.margin>
      </TableView>
   </children>
   <padding>
      <Insets left="15.0" right="15.0" />
   </padding>
</VBox>
