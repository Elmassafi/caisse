<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="200.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.121"
      fx:controller="com.fstg.caisse.controllers.CategoryStatistics">
    <Label prefHeight="43.0" prefWidth="193.0">les bénéfices de chaque catégorie :</Label>
    <LineChart fx:id="lineChart" layoutX="-1.0" prefHeight="200.0" prefWidth="500.0">
        <xAxis>
            <CategoryAxis side="BOTTOM" fx:id="x"/>
        </xAxis>
        <yAxis>
            <NumberAxis fx:id="y" side="LEFT"/>
        </yAxis>
    </LineChart>
</VBox>
